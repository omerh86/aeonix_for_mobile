<?xml version="1.0" encoding="utf-8" ?>
<ContentView xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="LinphoneXamarin.components.userControllers.CallListView"
              FlowDirection="{StaticResource FlowDirection}">
    <ContentView.Content>
        <Frame x:Name="infoFrame" Margin="0" Padding="5" BackgroundColor="Transparent">
            <Frame.GestureRecognizers>
                <TapGestureRecognizer
      Tapped="onCallListTapped"
      NumberOfTapsRequired="1"/>
            </Frame.GestureRecognizers>
            <Label x:Name="callsBtn"   TextColor="White"  FontSize="Large" Margin="3,0,3,0"/>
        </Frame>
    </ContentView.Content>
</ContentView>