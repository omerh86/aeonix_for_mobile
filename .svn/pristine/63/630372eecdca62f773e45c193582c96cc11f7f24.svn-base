<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="LinphoneXamarin.components.About"
             Title="About">
    <ContentPage.Content>
        <StackLayout x:Name="stack_layout" Margin="10">
            <Label Text="Aeonix Touch" Margin="0,0,0,10" HorizontalOptions="Center" TextColor="Black" FontSize="Large" 
                VerticalOptions="Center" />
            <Label  FontSize="Medium" Text="© 2021 Tadiran Telecom" FontAttributes="Bold"/>
            <Label x:Name="versionlbl" FontSize="Small" FontAttributes="Italic"/>
            <Label x:Name="AeonixVersionlbl" FontSize="Small" FontAttributes="Italic"/>
            <Label FontSize="Small"  x:Name="aboutTxt"/>

        </StackLayout>
    </ContentPage.Content>
</ContentPage>