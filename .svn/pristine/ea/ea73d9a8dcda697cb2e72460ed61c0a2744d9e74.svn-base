        public void onAudioChanged(MYAudioState state)
        {
            addLog("onAudioChanged: " + state);

            Device.BeginInvokeOnMainThread(() =>
            {
                switch (state)
                {
                    case MYAudioState.BT:
                        speakerBtn.Source = "bluetooth.png";
                        stopProximity();
                        break;
                    case MYAudioState.HEARPEACE:
                        speakerBtn.Source = "hearpeace.png";
                        startProximity();
                        break;
                    case MYAudioState.SPEAKER:
                        speakerBtn.Source = "speaker.png";
                        stopProximity();
                        break;
                }
            });

        }

